<h1> Users </h1>
<table>
  <thead>
  <tr>
    <th>CPF</th>
    <th>Email</th>
    <th></th>
  </tr>
  </thead>

  <% @usuarios.each do |user| %>
    <tr>
      <td><%= user.cpf %></td>
      <td><%= user.email %></td>
      <td><%= link_to "Show", user %></td>
    </tr>
  <%end%>
  <%= link_to "Ver vacinacaos", usuario_vacinacaos_path(:usuario_id => current_usuario,:id => @vacinacao) %>
  </br>
  <%= link_to "Ver vacinas", vacinas_path %>
  </tbody>
</table>